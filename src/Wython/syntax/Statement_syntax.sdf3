module Statement_syntax

imports
  
  ID_syntax
  Expression_syntax
  

context-free start-symbols
  Statement
  FunctionCall

context-free syntax
  Statement = FunctionCall
  
  
  FunctionCall.Call     = <<ID>(<FunctionArgs>)>
  FunctionArgs.Empty    = <>
  FunctionArgs.Single   = FunctionArg
  FunctionArgs.Multiple = <<FunctionArg>,<FunctionArgs>>
  FunctionArg           = Exp